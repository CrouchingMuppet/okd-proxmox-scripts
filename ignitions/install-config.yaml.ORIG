apiVersion: v1
baseDomain: localdomain
metadata:
  name: okd
compute:
  - name: worker
    replicas: 0
    platform: {}
    hyperthreading: Disabled
controlPlane:
  name: master
  replicas: 3
  platform: {}
  hyperthreading: Disabled
platform:
  none: {} # OKD requires this field to signal no cloud provider
pullSecret: '{"auths": "replaceme!"}'
sshKey: "ssh-ed25519 REPLACEME"
networking:
  networkType: OVNKubernetes
  clusterNetwork:
    - cidr: 10.128.0.0/14
      hostPrefix: 23
  serviceNetwork:
    - 172.30.0.0/16
  machineNetwork:
    - cidr: 192.168.1.0/24
fips: false

